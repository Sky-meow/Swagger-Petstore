<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="template.mapper.ApiResponseMapper">
  <resultMap id="BaseResultMap" type="template.entity.ApiResponse">
    <result column="code" jdbcType="INTEGER" property="code" />
    <result column="type" jdbcType="VARCHAR" property="type" />
    <result column="message" jdbcType="VARCHAR" property="message" />
  </resultMap>
  <insert id="insert" parameterType="template.entity.ApiResponse">
    insert into api_response (code, type, message
      )
    values (#{code,jdbcType=INTEGER}, #{type,jdbcType=VARCHAR}, #{message,jdbcType=VARCHAR}
      )
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select code, type, message
    from api_response
  </select>
</mapper>