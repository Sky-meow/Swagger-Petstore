<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="template.mapper.OrdersMapper">
  <resultMap id="BaseResultMap" type="template.entity.Orders">
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="pet_id" jdbcType="INTEGER" property="pet_id" />
    <result column="quantity" jdbcType="INTEGER" property="quantity" />
    <result column="ship_date" jdbcType="TIMESTAMP" property="ship_date" />
    <result column="status" jdbcType="VARCHAR" property="status" />
  </resultMap>
  <insert id="insert" keyColumn="id" keyProperty="id" parameterType="template.entity.Orders" useGeneratedKeys="true">
    insert into orders (pet_id, quantity, ship_date, 
      status)
    values (#{pet_id,jdbcType=INTEGER}, #{quantity,jdbcType=INTEGER}, #{ship_date,jdbcType=TIMESTAMP}, 
      #{status,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select id, pet_id, quantity, ship_date, status
    from orders
  </select>
</mapper>